<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ryan.view_siso">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FindActivity"
            android:label="@string/title_activity_find"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.FindStuidActivity"
            android:label="@string/title_activity_find_stuid"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.FindClassActivity"
            android:label="@string/title_activity_find_class"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.FindNameActivity"
            android:label="@string/title_activity_find_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.FindClassUserActivity"
            android:label="@string/title_activity_find_class_user"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
